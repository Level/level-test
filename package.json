{
  "name": "level-test",
  "version": "8.0.0",
  "description": "Inject temporary and isolated level stores (leveldown, level-js, memdown or custom) into your tests.",
  "author": "Dominic Tarr <dominic.tarr@gmail.com> (http://dominictarr.com)",
  "license": "MIT",
  "scripts": {
    "test": "standard && hallmark && (nyc -s node test/node.js | faucet) && nyc report",
    "coverage": "nyc report -r lcovonly",
    "test-browsers-local": "airtap test/browser.js",
    "hallmark": "hallmark --fix",
    "dependency-check": "dependency-check . browser.js test/*.js",
    "prepublishOnly": "npm run dependency-check"
  },
  "browser": {
    "leveldown": "level-js",
    "./location.js": "./location-browser.js"
  },
  "dependencies": {
    "encoding-down": "^6.2.0",
    "level-compose": "^0.0.2",
    "level-js": "^5.0.0",
    "levelup": "^4.3.1",
    "memdown": "^5.1.0",
    "tempy": "^1.0.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "abstract-leveldown": "^6.0.0",
    "airtap": "^4.0.3",
    "airtap-playwright": "^1.0.1",
    "dependency-check": "^3.3.0",
    "faucet": "^0.0.1",
    "hallmark": "^3.1.0",
    "jsondown": "^1.0.0",
    "level-community": "^3.0.0",
    "level-packager": "^5.0.3",
    "nyc": "^15.0.0",
    "reachdown": "^1.0.0",
    "standard": "^16.0.3",
    "tape": "^5.0.1"
  },
  "optionalDependencies": {
    "leveldown": "^5.2.1"
  },
  "hallmark": {
    "community": "level-community",
    "ignore": [
      "README.md"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Level/level-test.git"
  },
  "homepage": "https://github.com/Level/level-test",
  "keywords": [
    "level"
  ],
  "engines": {
    "node": ">=8"
  }
}
