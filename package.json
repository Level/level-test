{
  "name": "level-test",
  "version": "7.1.0",
  "description": "Inject temporary and isolated level stores (leveldown, level-js, memdown or custom) into your tests.",
  "author": "Dominic Tarr <dominic.tarr@gmail.com> (http://dominictarr.com)",
  "license": "MIT",
  "scripts": {
    "test": "standard && hallmark && nyc node test/node.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test-browser-local": "airtap --local test/browser.js",
    "hallmark": "hallmark --fix",
    "dependency-check": "dependency-check . browser.js test/*.js",
    "prepublishOnly": "npm run dependency-check"
  },
  "browser": {
    "leveldown": "level-js",
    "./location.js": "./location-browser.js"
  },
  "dependencies": {
    "encoding-down": "~6.1.0",
    "level-compose": "^0.0.2",
    "level-js": "~4.0.0",
    "levelup": "~4.1.0",
    "memdown": "~4.1.0",
    "tempy": "^0.3.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "abstract-leveldown": "^6.0.0",
    "airtap": "^2.0.0",
    "coveralls": "^3.0.2",
    "dependency-check": "^4.0.0",
    "hallmark": "^1.0.0",
    "jsondown": "^1.0.0",
    "level-community": "^3.0.0",
    "nyc": "^14.0.0",
    "standard": "^13.0.1",
    "tape": "^4.8.0"
  },
  "optionalDependencies": {
    "leveldown": "~5.1.0"
  },
  "hallmark": {
    "community": "level-community",
    "ignore": [
      "README.md"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Level/level-test.git"
  },
  "homepage": "https://github.com/Level/level-test",
  "keywords": [
    "level"
  ],
  "engines": {
    "node": ">=8"
  }
}
