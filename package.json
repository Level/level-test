{
  "name": "level-test",
  "version": "5.0.0",
  "description": "get a clean levelup instance for testing.",
  "author": "Dominic Tarr <dominic.tarr@gmail.com> (http://dominictarr.com)",
  "license": "MIT",
  "scripts": {
    "test": "standard && hallmark && nyc node test/node.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test-browser-local": "airtap --local test/browser.js",
    "hallmark": "hallmark --fix",
    "dependency-check": "dependency-check . browser.js test/*.js",
    "prepublishOnly": "npm run dependency-check"
  },
  "browser": "browser.js",
  "dependencies": {
    "level-js": "~4.0.0",
    "level-packager": "~5.0.0",
    "memdown": "~4.0.0",
    "tempy": "^0.2.1",
    "uuid": "^3.3.2",
    "xtend": "~4.0.0"
  },
  "devDependencies": {
    "abstract-leveldown": "^6.0.0",
    "airtap": "^2.0.0",
    "coveralls": "^3.0.2",
    "dependency-check": "^3.3.0",
    "hallmark": "^0.1.0",
    "jsondown": "^1.0.0",
    "level-community": "^3.0.0",
    "nyc": "^13.2.0",
    "standard": "^12.0.0",
    "tape": "^4.8.0"
  },
  "optionalDependencies": {
    "leveldown": "~5.0.0"
  },
  "hallmark": {
    "community": "level-community",
    "ignore": [
      "README.md"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Level/level-test.git"
  },
  "homepage": "https://github.com/Level/level-test",
  "keywords": [
    "level"
  ],
  "engines": {
    "node": ">=6"
  }
}
